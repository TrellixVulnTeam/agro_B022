\" class=\"mb-4 mr-4\">+ Добавить продукт</v-btn>\n\n    <folder parent_id=\"0\" id=\"\"></folder>\n\n    <v-container class=\"tree-box\" fluid>\n      <v-row class=\"tree-header\">\n        <v-col cols=\"4\">\n          Наименование\n        </v-col>\n        <v-col cols=\"1\">\n          Код\n        </v-col>\n        <v-col cols=\"5\">\n          Описание\n        </v-col>\n        <v-col cols=\"2\">\n          Файл\n        </v-col>\n      </v-row>\n      <v-row class=\"tree-row tree-folders\" v-for=\"folder in folders\" :key=\"folder.id\">\n        <v-col cols=\"4\">\n          <router-link :to=\"'/products/folder/' + folder.id\">\n            <v-icon color=\"blue\">folder</v-icon>\n            {{ folder.folder_name }}\n          </router-link>\n        </v-col>\n        <v-col cols=\"1\">\n          {{ folder.id }}\n        </v-col>\n        <v-col cols=\"5\">\n          ...\n        </v-col>\n        <v-col cols=\"2\">\n          ...\n        </v-col>\n      </v-row>\n\n      <v-row class=\"tree-row tree-folders\" v-for=\"item in items\" :key=\"item.id\">\n        <v-col cols=\"4\">\n          <router-link :to=\"'/products/' + item.id\">\n            <v-icon>mdi-file-outline</v-icon>\n            {{ item.name }}\n          </router-link>\n        </v-col>\n        <v-col cols=\"1\">\n          {{ item.classification_number.slice(0, 7) }}\n        </v-col>\n        <v-col cols=\"5\">\n          {{ item.description.slice(0, 40) }}\n        </v-col>\n        <v-col cols=\"2\">\n          ...\n        </v-col>\n      </v-row>\n    </v-container>\n    <div class=\"text-left mt-4\">\n      <v-pagination\n        v-model=\"paginator.current_pages\"\n        :length=\"paginator.total_pages\"\n        @input=\"getProducts()\"\n      ></v-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nconst folder = () => import('@/components/folder')\nexport default {\n  name: 'Products',\n  data() {\n    return {\n    }\n  },\n  components: {\n    folder\n  },\n  methods: {\n    getProducts() {\n      this.$store.dispatch('getProducts', 0)\n    }\n  },\n  computed: {\n    loading () {\n      return this.$store.getters.loading\n    },\n    folders() {\n      return this.$store.getters.products.folders\n    },\n    items() {\n      return this.$store.getters.products.data\n    },\n    paginator() {\n      return this.$store.getters.products.paginator\n    }\n  },\n  created() {\n    this.getProducts()\n  }\n}\n</script>\n\n<style lang=\"scss\">\n</style>\n"],"sourceRoot":"src/views"}]}