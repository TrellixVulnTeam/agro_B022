AA,cAAA,EAAA,KAAA,SAAA;AACA,KAxBA;AAyBA,IAAA,YAzBA,wBAyBA,MAzBA,EAyBA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA,MAAA;AACA,KA5BA;AA6BA,IAAA,WA7BA,yBA6BA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,EAAA;AACA;AAhCA,GARA;AA0CA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,OAAA;AACA,KAHA;AAIA,IAAA,KAJA,mBAIA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,IAAA;AACA,KANA;AAOA,IAAA,SAPA,uBAOA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,SAAA;AACA,KATA;AAUA,IAAA,MAVA,oBAUA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,MAAA;AACA,KAZA;AAaA,IAAA,YAbA,0BAaA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,YAAA;AACA,KAfA;AAgBA,IAAA,gBAhBA,8BAgBA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,CAAA,IAAA;AACA,KAlBA;AAmBA,IAAA,OAnBA,qBAmBA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA;AACA;AArBA,GA1CA;AAiEA,EAAA,OAjEA,qBAiEA;AACA,SAAA,aAAA,CAAA,KAAA,SAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,qBAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,iBAAA;AACA,GArEA;AAsEA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,mBACA;AAAA;;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,GAAA,CAAA,IAAA,GAAA,eAAA,CAAA,GAAA,EAAA,KAAA,CAAA,gBAAA,CAAA;AACA,OAFA;AAGA,KALA;AAMA,IAAA,MANA,oBAMA;AACA,WAAA,aAAA,CAAA,KAAA,SAAA;AACA;AARA;AAtEA,CAAA","sourcesContent":["<template>\n  <div>\n    <div @click=\"$router.back()\" class=\"back-btn\">\n      <v-icon class=\"ml-0\">chevron_left</v-icon>назад\n    </div>\n    <h1 class=\"display-1\">Склады</h1>\n    <v-divider class=\"mt-2 mb-8\"></v-divider>\n    <v-btn @click=\"$router.push('/warehouse/new')\" depressed color=\"light-grey\" class=\"mb-4 mr-4\">+ Добавить склад</v-btn>\n\n    <v-dialog\n      v-model=\"folderDialog\"\n      persistent\n      max-width=\"600px\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-btn\n          depressed\n          color=\"light-grey\"\n          class=\"mb-4\"\n          v-bind=\"attrs\"\n          v-on=\"on\"\n        >\n          + Добавить папку\n        </v-btn>\n      </template>\n      <v-card>\n        <v-card-title>\n          <span class=\"text-h5\" v-if=\"folder.id\">Редактирование папки</span>\n          <span class=\"text-h5\" v-else><h1 class=\"display-1\">Новая папка</h1></span>\n        </v-card-title>\n        <v-divider class=\"mb-4\"></v-divider>\n        <v-card-text class=\"pb-0\">\n\n          <v-text-field label=\"Наименование\" outlined v-model=\"folder.folder_name\"></v-text-field>\n\n          <v-select\n            v-if=\"!folder.id\"\n            :items=\"folderModels\"\n            v-model=\"folder.model\"\n            outlined\n            label=\"Модель папки\"\n            item-text=\"model_name\"\n          ></v-select>\n\n        </v-card-text>\n        <v-card-actions class=\"pa-4\">\n          <v-spacer></v-spacer>\n          <div v-if=\"folder.id\">\n            <v-btn depressed color=\"light-grey\" @click=\"updateFolder\" class=\"mr-3\">Обновить</v-btn>\n            <v-btn depressed color=\"light-grey\" @click=\"closeFolder\" class=\"mr-3\">Закрыть</v-btn>\n          </div>\n          <div v-else>\n            <v-btn depressed color=\"light-grey\" @click=\"createFolder\" class=\"mr-3\">Создать</v-btn>\n            <v-btn depressed color=\"light-grey\" @click=\"closeFolder\" class=\"mr-3\">Закрыть</v-btn>\n          </div>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n\n    <v-progress-linear indeterminate v-if=\"loading\"></v-progress-linear>\n    <v-container class=\"tree-box\" fluid>\n      <v-row class=\"tree-header\">\n        <v-col cols=\"3\">\n          Наименование\n        </v-col>\n        <v-col cols=\"2\">\n          Размер участка\n        </v-col>\n        <v-col cols=\"4\">\n          Описание\n        </v-col>\n        <v-col cols=\"2\">\n          Тип сада\n        </v-col>\n        <v-col cols=\"1\">\n        </v-col>\n      </v-row>\n      <v-row class=\"tree-row tree-folders\" v-for=\"folder in folders\" :key=\"folder.id\">\n        <v-col cols=\"3\" @click=\"openFolder(folder.id)\">\n          <span class=\"folder-name\">\n            <v-icon color=\"blue\">folder</v-icon>\n            {{ folder.folder_name }}\n          </span>\n        </v-col>\n        <v-col cols=\"2\">\n        </v-col>\n        <v-col cols=\"4\">\n        </v-col>\n        <v-col cols=\"2\">\n        </v-col>\n        <v-col cols=\"1\" class=\"text-right\">\n          <div class=\"actions\">\n            <v-icon\n              small\n              class=\"mr-2\"\n              @click=\"editFolder(folder)\"\n            >\n              mdi-pencil\n            </v-icon>\n            <v-icon\n              small\n              @click=\"deleteFolder(folder)\"\n            >\n              mdi-delete\n            </v-icon>\n          </div>\n        </v-col>\n      </v-row>\n\n      <v-row class=\"tree-row tree-folders\" v-for=\"item in items\" :key=\"item.id\">\n        <v-col cols=\"3\">\n          <v-icon>mdi-file-outline</v-icon>\n          {{ item.name }}\n        </v-col>\n        <v-col cols=\"2\">\n          {{ item.volume }} га\n        </v-col>\n        <v-col cols=\"4\">\n          {{ item.description.slice(0, 40) }}\n        </v-col>\n        <v-col cols=\"2\">\n          {{ item.type }}\n        </v-col>\n        <v-col cols=\"1\" class=\"text-right actions\">\n          <v-icon\n            small\n            class=\"mr-2\"\n            @click=\"editItem(item)\"\n          >\n            mdi-pencil\n          </v-icon>\n          <v-icon\n            small\n            @click=\"deleteItem(item)\"\n          >\n            mdi-delete\n          </v-icon>\n        </v-col>\n      </v-row>\n    </v-container>\n    <div class=\"text-left mt-4\">\n      <v-pagination\n        depressed\n        v-model=\"paginator.current_pages\"\n        :length=\"paginator.total_pages\"\n        @input=\"getWarehouses()\"\n      ></v-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { nameTheWrhsType } from '@/helpers/helpers.js'\nexport default {\n  name: 'Warehouses',\n  data() {\n    return {\n      folderDialog: false\n    }\n  },\n  props: [ 'folder_id' ],\n  methods: {\n    openFolder(id) {\n      this.$router.push('/warehouses/' + id)\n    },\n    getWarehouses(id) {\n      this.$store.dispatch('getWarehouses', id)\n    },\n    editFolder(folder) {\n      this.folderDialog = true\n      this.$store.dispatch('getFolder', folder)\n    },\n    editItem(item) {\n      this.$router.push('/warehouse/' + item.id)\n    },\n    deleteItem(item) {\n      this.$store.dispatch('deleteWarehouse', item)\n    },\n    updateFolder () {\n      this.folderDialog = false\n      this.$store.dispatch('updateFolder')\n    },\n    createFolder () {\n      this.folderDialog = false\n      this.$store.dispatch('createFolder', this.folder_id)\n    },\n    deleteFolder (folder) {\n      this.folderDialog = false\n      this.$store.dispatch('deleteFolder', folder)\n    },\n    closeFolder () {\n      this.folderDialog = false\n      this.$store.dispatch('getFolder', {})\n    }\n  },\n  computed: {\n    folders() {\n      return this.$store.getters.warehouses.folders\n    },\n    items() {\n      return this.$store.getters.warehouses.data\n    },\n    paginator() {\n      return this.$store.getters.warehouses.paginator\n    },\n    folder() {\n      return this.$store.getters.folder\n    },\n    folderModels() {\n      return this.$store.getters.folderModels\n    },\n    warehouses_types() {\n      return this.$store.getters.warehouses_types.data\n    },\n    loading () {\n      return this.$store.getters.loading\n    }\n  },\n  created() {\n    this.getWarehouses(this.folder_id)\n    this.$store.dispatch('getWarehouses_types')\n    this.$store.dispatch('getFolderModels')\n  },\n  watch:{\n    items() {\n      this.items.forEach(wrh => {\n        wrh.type = nameTheWrhsType(wrh, this.warehouses_types)\n      })\n    },\n    $route() {\n      this.getWarehouses(this.folder_id)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n</style>\n"],"sourceRoot":"src/views"}]}