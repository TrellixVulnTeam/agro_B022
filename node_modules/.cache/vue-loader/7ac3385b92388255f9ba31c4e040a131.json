    </v-row>\n        <v-row>\n          <v-col cols=\"9\">\n            <v-text-field label=\"Файл\" outlined v-model=\"product.files\"></v-text-field>\n          </v-col>\n          <v-col cols=\"3\">\n            <v-text-field label=\"Родитель\" outlined v-model.number=\"product.parent_id\"></v-text-field>\n          </v-col>\n        </v-row>\n        <v-row>\n          <v-col cols=\"12\">\n            <v-textarea label=\"Описание\" outlined v-model=\"product.description\"></v-textarea>\n          </v-col>\n        </v-row>\n      </v-col>\n      <v-col cols=\"5\">\n        <vue-dropzone\n          ref=\"myVueDropzone\"\n          id=\"dropzone\"\n          class=\"dropzone\"\n          :options=\"dropzoneOptions\"\n        ></vue-dropzone>\n      </v-col>\n    </v-row>\n    <div v-if=\"product.id\">\n      <v-btn x-large depressed color=\"light-grey\" @click=\"updateProduct\" class=\"mr-3\">Обновить</v-btn>\n      <v-btn x-large depressed color=\"light-red\" @click=\"deleteProduct\" class=\"\">Удалить продукцию</v-btn>\n    </div>\n    <div v-else>\n      <v-btn x-large depressed color=\"light-grey\" @click=\"createProduct\" class=\"mr-3\">Создать</v-btn>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport vue2Dropzone from 'vue2-dropzone'\nimport 'vue2-dropzone/dist/vue2Dropzone.min.css'\nexport default {\n  name: 'Product',\n  props: ['id'],\n  components: {\n    vueDropzone: vue2Dropzone\n  },\n  data() {\n    return {\n      dropzoneOptions: {\n          url: '#',\n          thumbnailWidth: 150,\n          thumbnailHeight: 150,\n          maxFilesize: 1,\n          dictDefaultMessage: \"<i class='v-icon notranslate mdi mdi-cloud-upload theme--light'></i> ЗАГРУЗИТЬ ФАЙЛЫ\",\n          headers: { \"Header\": \"header value\" }\n      }\n    }\n  },\n  methods: {\n    updateProduct () {\n      this.$store.dispatch('updateProduct')\n    },\n    createProduct () {\n      this.$store.dispatch('createProduct')\n    },\n    deleteProduct () {\n      this.$store.dispatch('deleteProduct', this.product)\n    }\n  },\n  computed: {\n    loading () {\n      return this.$store.getters.loading\n    },\n    product() {\n      return this.$store.getters.product\n    }\n  },\n  created() {\n    if (!isNaN(this.id)) {\n      this.$store.dispatch('getProduct', this.id)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n  .form-grid {\n    margin-bottom: 0px;\n    .row {\n      margin-top: 0;\n    }\n    .col {\n      padding-top: 0;\n      padding-bottom: 0;\n    }\n  }\n  .dropzone {\n    min-height: 300px;\n  }\n</style>\n"]}]}